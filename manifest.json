{
  "name": "Event Catcher",
  "description": "Catches events from emails instantly.",
  "version": "1.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["scripts/gmail.js"],
      "matches": [
        "https://mail.google.com/mail/u/*"
      ]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png",
    "512": "images/icon-512.png"
  }
}